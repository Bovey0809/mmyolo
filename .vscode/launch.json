  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "mmyolo train",
      "type": "python",
      "request": "launch",
      "program": "tools/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "${file}"
      ],
      "env": {
        "CUDA_LAUNCH_BLOCKING": "1"
      }
    },
    {
      "name": "vis pipeline",
      "type": "python",
      "request": "launch",
      "program": "tools/analysis_tools/browse_dataset.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "${file}",
        "-n",
        "20",
        "--out-dir",
        "vis/origin",
        "-m",
        "pipeline",
        "-p",
        "val"
      ]
    },
    {
      "name": "yolo pose test",
      "type": "python",
      "request": "launch",
      "program": "tools/test.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "configs/yolox/yoloxkptonly_s_8xb8-100e_coco6280.py",
        // "configs/yolox/yoloxkpt_s_8xb8-300e_coco_mini_debug.py",
        // "work_dirs/yoloxkpt_s_8xb8-300e_coco/yoloxkpt_s_8xb8-300e_coco.py",
        // "work_dirs/yoloxkpt_s_8xb8-300e_coco/best_coco/bbox_mAP_epoch_100.pth",
        "/home/houbowei/mmyolo/work_dirs/yoloxkptonly_s_8xb32-300e_coco/epoch_300.pth"
        // "/home/houbowei/yolox-pose/YOLOX_outputs/kpt_anno_separate_head/best_ckpt_mmyolo.pth"
        // "--show",
        // "--json-prefix",
        // "bug"
      ]
    },
    {
      "name": "export to onnx",
      "type": "python",
      "request": "launch",
      "program": "./projects/easydeploy/tools/export.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "configs/yolox/yoloxkpt_s_8xb8-300e_coco_mini_debug.py",
        "work_dirs/yoloxkpt_s_8xb8-300e_coco/best_coco/bbox_mAP_epoch_100.pth",
        "--simplify",
        "--model-only"
      ]
    },
    {
      "name": "convert yolox to mmyolo",
      "type": "python",
      "request": "launch",
      "program": "tools/model_converters/yolox_to_mmyolo.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "--src",
        "~/yolox-pose/YOLOX_outputs/kpt_anno_separate_head/best_ckpt.pth",
        "--dst",
        "~/yolox-pose/YOLOX_outputs/kpt_anno_separate_head/best_ckpt_mmyolo.pth",
      ]
    }
  ]
}
